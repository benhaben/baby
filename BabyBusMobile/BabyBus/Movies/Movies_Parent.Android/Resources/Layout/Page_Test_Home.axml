<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:scrollbars="vertical"
    android:background="@color/bb_gray3">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <LinearLayout
            android:id="@+id/parent_gm"
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_marginTop="0dp"
            android:layout_height="150dp">
            <android.support.v4.view.ViewPager
                android:id="@+id/pager"
                android:layout_width="fill_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:background="@drawable/bar_slide_1" />
            <babybus.droid.viewpagerindicator.CirclePageIndicator
                android:id="@+id/indicator"
                android:padding="2dp"
                android:layout_height="wrap_content"
                android:layout_width="fill_parent" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginLeft="4.0dip"
            android:layout_marginTop="2.0dip"
            android:layout_marginRight="4.0dip"
            android:isScrollContainer="true"
            android:background="@android:color/background_light"
            android:paddingBottom="5dp"
            android:id="@+id/linearLayout18">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:orientation="horizontal"
                android:gravity="left|center_vertical"
                android:id="@+id/linearLayout10">
                <ImageView
                    android:layout_width="10dp"
                    android:layout_height="25dp"
                    android:background="@android:color/holo_blue_light"
                    android:id="@+id/imageView2" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:gravity="center_vertical"
                    android:text="体格测评"
                    android:textSize="18dp"
                    android:id="@+id/textView2" />
            </LinearLayout>
            <ImageView
                android:layout_margin="5dp"
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:background="@drawable/icon_test_physical"
                android:id="@+id/image_testprepare" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:id="@+id/linearLayout15">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:id="@+id/linearLayout16">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/button_1_1"
                        android:text="我的宝宝"
                        android:textSize="16dp"
                        android:paddingLeft="15dp"
                        android:paddingRight="15dp"
                        android:paddingBottom="2dp"
                        android:paddingTop="2dp"
                        android:background="@drawable/button_bar_blue"
                        android:textColor="@android:color/holo_blue_light" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:id="@+id/linearLayout17">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/button_1_2"
                        android:text="相关建议"
                        android:textSize="16dp"
                        android:paddingLeft="15dp"
                        android:paddingRight="15dp"
                        android:paddingBottom="2dp"
                        android:paddingTop="2dp"
                        android:background="@drawable/button_bar_blue"
                        android:textColor="@android:color/holo_blue_light" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginLeft="4.0dip"
            android:layout_marginTop="5.0dip"
            android:layout_marginRight="4.0dip"
            android:isScrollContainer="true"
            android:background="@android:color/background_light"
            android:paddingBottom="5dp"
            android:id="@+id/linearLayout9">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:orientation="horizontal"
                android:gravity="left|center_vertical"
                android:id="@+id/linearLayout1">
                <ImageView
                    android:layout_width="10dp"
                    android:layout_height="25dp"
                    android:background="@color/bb_orange"
                    android:id="@+id/imageView1" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:gravity="center_vertical"
                    android:text="多元智能"
                    android:textSize="18dp"
                    android:id="@+id/textView1" />
            </LinearLayout>
            <ImageView
                android:layout_margin="5dp"
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:background="@drawable/icon_test_multiple_ntelligence"
                android:id="@+id/image_testmultiple" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:id="@+id/linearLayout6">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:id="@+id/linearLayout7">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/button_2_1"
                        android:text="我的宝宝"
                        android:paddingLeft="15dp"
                        android:textSize="16dp"
                        android:paddingRight="15dp"
                        android:paddingBottom="2dp"
                        android:paddingTop="2dp"
                        android:background="@drawable/button_bar_orange"
                        android:textColor="@color/bb_orange" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:id="@+id/linearLayout8">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/button_2_2"
                        android:text="相关建议"
                        android:paddingLeft="15dp"
                        android:textSize="16dp"
                        android:paddingRight="15dp"
                        android:paddingBottom="2dp"
                        android:paddingTop="2dp"
                        android:background="@drawable/button_bar_orange"
                        android:textColor="@color/bb_orange" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginLeft="4.0dip"
            android:layout_marginTop="5.0dip"
            android:layout_marginRight="4.0dip"
            android:isScrollContainer="true"
            android:background="@android:color/background_light"
            android:paddingBottom="5dp"
            android:id="@+id/linearLayout10">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:orientation="horizontal"
                android:gravity="left|center_vertical"
                android:id="@+id/linearLayout1">
                <ImageView
                    android:layout_width="10dp"
                    android:layout_height="25dp"
                    android:background="@color/bb_green"
                    android:id="@+id/imageView1" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:gravity="center_vertical"
                    android:text="气质测试"
                    android:textSize="18dp"
                    android:id="@+id/textView1" />
            </LinearLayout>
            <ImageView
                android:layout_margin="5dp"
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:background="@drawable/icon_test_bar_qizhi"
                android:id="@+id/image_temperament" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:id="@+id/linearLayout6">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:id="@+id/linearLayout7">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/button_3_1"
                        android:text="我的宝宝"
                        android:paddingLeft="15dp"
                        android:textSize="16dp"
                        android:paddingRight="15dp"
                        android:paddingBottom="2dp"
                        android:paddingTop="2dp"
                        android:background="@drawable/button_bar_green"
                        android:textColor="@color/bb_green" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:id="@+id/linearLayout8">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/button_3_2"
                        android:text="相关建议"
                        android:paddingLeft="15dp"
                        android:textSize="16dp"
                        android:paddingRight="15dp"
                        android:paddingBottom="2dp"
                        android:paddingTop="2dp"
                        android:background="@drawable/button_bar_green"
                        android:textColor="@color/bb_green" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</ScrollView>